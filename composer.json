{
    "name": "yugo/moota",
    "description": "Unofficial Moota.co package for Laravel framework",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Yugo",
            "email": "dedy.yugo.purwanto@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Yugo\\Moota\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yugo\\Moota\\Providers\\MootaServiceProvider"
